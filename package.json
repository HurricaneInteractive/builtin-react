{
    "name": "builtin-react",
    "version": "0.2.0",
    "description": "React Component library for rapid prototyping",
    "main": "build/index.js",
    "scripts": {
        "build": "babel lib --extensions '.ts,.tsx,.js,.jsx' --out-dir build",
        "build:watch": "babel lib -w --extensions '.ts,.tsx,.js,.jsx' --out-dir build",
        "lint": "eslint lib/**; exit 0",
        "lint:watch": "esw -w lib/**",
        "prepublish": "npm run build"
    },
    "author": "Winking Owl <adriaan@winkingowl.digital>",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.47",
        "@babel/core": "^7.0.0-beta.47",
        "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
        "@babel/plugin-transform-typescript": "^7.0.0-beta.47",
        "@babel/preset-env": "^7.0.0-beta.47",
        "@babel/preset-react": "^7.0.0-beta.47",
        "@babel/preset-typescript": "^7.0.0-beta.47",
        "@types/classnames": "^2.2.3",
        "@types/react": "^16.3.14",
        "eslint": "^4.19.1",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.8.2",
        "eslint-watch": "^3.1.4",
        "prop-types": "^15.6.1",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "typescript": "^2.8.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HurricaneInteractive/builtin-react"
    },
    "bugs": {
        "url": "https://github.com/HurricaneInteractive/builtin-react/issues"
    },
    "deprecated": false,
    "homepage": "https://github.com/HurricaneInteractive/builtin-react",
    "dependencies": {
        "classnames": "^2.2.5",
        "styled-components": "^3.2.6",
        "polished": "^1.9.2"
    },
    "keywords": [
        "react",
        "components",
        "prototyping",
        "reusable",
        "builtin",
        "typescript"
    ]
}
